{
  "name": "pico-y-placa-predictor",
  "version": "1.0.0",
  "description": "A command-line application that predicts whether a vehicle can be on the road based on Quito-Ecuador's \"Pico y Placa\" restrictions.",
  "main": "src/index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "start": "node src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vikus7/pico-y-placa-predictor.git"
  },
  "keywords": [
    "pico-y-placa",
    "quito",
    "ecuador",
    "traffic",
    "predictor"
  ],
  "author": "Victor Rodriguez",
  "license": "MIT",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/Vikus7/pico-y-placa-predictor/issues"
  },
  "homepage": "https://github.com/Vikus7/pico-y-placa-predictor#readme",
  "devDependencies": {
    "jest": "^30.2.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  }
}
